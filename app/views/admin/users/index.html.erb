<h1>Listing users</h1>

<table>
	<tr>
		<th>Email</th>
		<th></th>
		<th></th>
	</tr>

<% @users.each do |user| %>
	<tr>
		<td><%= link_to user.email, edit_admin_user_path(user) %></td>
    <td><%= link_to 'Delete', admin_user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
	</tr>
<% end %>

</table>

<br />
<%= link_to 'New User', new_admin_user_path %>

